@PACKAGE_INIT@

include(CMakeFindDependencyMacro)

# Find required dependencies
find_dependency(GMP REQUIRED)
find_dependency(MPFR REQUIRED)

# Include the exported targets
include("${CMAKE_CURRENT_LIST_DIR}/libadicTargets.cmake")

# Provide the libadic::adic target
if(NOT TARGET libadic::adic)
    message(FATAL_ERROR "libadic::adic target not found")
endif()

# Set variables for compatibility
set(libadic_FOUND TRUE)
set(libadic_LIBRARIES libadic::adic)
set(libadic_INCLUDE_DIRS "@PACKAGE_CMAKE_INSTALL_INCLUDEDIR@")

check_required_components(libadic)