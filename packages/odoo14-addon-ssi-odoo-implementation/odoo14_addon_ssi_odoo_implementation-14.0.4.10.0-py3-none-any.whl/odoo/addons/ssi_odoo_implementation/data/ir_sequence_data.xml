<odoo>
<data noupdate="1">
<record id="sequence_odoo_implementation" model="ir.sequence">
    <field name="name">Odoo Implementation</field>
    <field name="code">odoo_implementation</field>
    <field name="prefix">OI/%(range_y)s/</field>
    <field eval="1" name="number_next" />
    <field eval="1" name="number_increment" />
    <field name="padding" eval="6" />
    <field name="use_date_range" eval="1" />
    <field eval="False" name="company_id" />
</record>

