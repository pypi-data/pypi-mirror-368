    <field name="use_date_range" eval="1" />
    <field eval="False" name="company_id" />
</record>

<record id="sequence_odoo_feature_issue" model="ir.sequence">
    <field name="name">Odoo Feature Issue</field>
    <field name="code">odoo_feature_issue</field>
    <field name="prefix">OFS/%(range_y)s/</field>
    <field eval="1" name="number_next" />
    <field eval="1" name="number_increment" />
    <field name="padding" eval="6" />
    <field name="use_date_range" eval="1" />
    <field eval="False" name="company_id" />
