#!python

"""
Executable to make rivet plots from generated python scripts

"""

import sys

from contur.run.run_mkplots import main
from contur.run.arg_utils import get_args
import contur.config

if __name__ == "__main__":

    # specify the default log name for this exec
    contur.config.logfile_name="contur_plot.log"
    cl_args = get_args(sys.argv[1:],'mkhtml')

    main(cl_args)

