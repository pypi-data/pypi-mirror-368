# Used with param_file.dat

read FRModel.model

set /Herwig/FRModel/Particles/chi:NominalMass {MCHI} 
set /Herwig/FRModel/Particles/ZB:NominalMass {MZB}
set /Herwig/FRModel/Particles/hB:NominalMass {MHB} 
set /Herwig/FRModel/FRModel:sth {sintheta}
set /Herwig/FRModel/FRModel:gB {gB}

set /Herwig/Cuts/JetKtCut:MinKT 150*GeV

cd /Herwig/NewPhysics
insert HPConstructor:Outgoing 0 /Herwig/FRModel/Particles/hB
insert HPConstructor:Outgoing 0 /Herwig/FRModel/Particles/ZB
# insert HPConstructor:Outgoing 0 /Herwig/FRModel/Particles/chi

insert ResConstructor:Intermediates 0 /Herwig/FRModel/Particles/ZB
insert ResConstructor:Intermediates 0 /Herwig/FRModel/Particles/hB
## all SM particles are possible outgoing from resonance
insert ResConstructor:Outgoing 0 /Herwig/Particles/u
insert ResConstructor:Outgoing 0 /Herwig/Particles/ubar
insert ResConstructor:Outgoing 0 /Herwig/Particles/d
insert ResConstructor:Outgoing 0 /Herwig/Particles/dbar
insert ResConstructor:Outgoing 0 /Herwig/Particles/c
insert ResConstructor:Outgoing 0 /Herwig/Particles/cbar
insert ResConstructor:Outgoing 0 /Herwig/Particles/s
insert ResConstructor:Outgoing 0 /Herwig/Particles/sbar
insert ResConstructor:Outgoing 0 /Herwig/Particles/b
insert ResConstructor:Outgoing 0 /Herwig/Particles/bbar
insert ResConstructor:Outgoing 0 /Herwig/Particles/t
insert ResConstructor:Outgoing 0 /Herwig/Particles/tbar
insert ResConstructor:Outgoing 0 /Herwig/Particles/g
insert ResConstructor:Outgoing 0 /Herwig/Particles/e+
insert ResConstructor:Outgoing 0 /Herwig/Particles/e-
insert ResConstructor:Outgoing 0 /Herwig/Particles/mu+
insert ResConstructor:Outgoing 0 /Herwig/Particles/mu-
insert ResConstructor:Outgoing 0 /Herwig/Particles/tau+
insert ResConstructor:Outgoing 0 /Herwig/Particles/tau-
insert ResConstructor:Outgoing 0 /Herwig/Particles/nu_e
insert ResConstructor:Outgoing 0 /Herwig/Particles/nu_ebar
