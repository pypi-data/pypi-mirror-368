read FRModel.model 
set /Herwig/FRModel/Particles/Y1:NominalMass {mY1}*GeV 
set /Herwig/FRModel/Particles/Xm:NominalMass {mXm}*GeV 
set /Herwig/FRModel/FRModel:gYXm {gYXm}
set /Herwig/FRModel/FRModel:gYq  {gYq}

cd /Herwig/NewPhysics
insert HPConstructor:Outgoing 0 /Herwig/FRModel/Particles/Y1
insert HPConstructor:Outgoing 0 /Herwig/FRModel/Particles/Xm
insert ResConstructor:Intermediates 0 /Herwig/FRModel/Particles/Y1

