read FRModel.model

# Set the BSM parameters here (see FRModel.model)
# set /Herwig/FRModel/FRModel:mmA0 120000
set /Herwig/FRModel/FRModel:mmh {mmh}*GeV
set /Herwig/FRModel/FRModel:mmH0 {mmH0}
set /Herwig/FRModel/FRModel:mmA0 {mmA0}
set /Herwig/FRModel/FRModel:mmHch {mmHch}
set /Herwig/FRModel/FRModel:lamL {lamL}
set /Herwig/FRModel/FRModel:lam2 {lam2}
# etc...


####################################
#
# Modify the required process here
#
####################################

cd /Herwig/NewPhysics

insert HPConstructor:Outgoing 0 /Herwig/FRModel/Particles/~H0
insert HPConstructor:Outgoing 0 /Herwig/FRModel/Particles/~A0
insert HPConstructor:Outgoing 0 /Herwig/FRModel/Particles/~H+
insert HPConstructor:Outgoing 0 /Herwig/FRModel/Particles/~H-

set HPConstructor:Processes SingleParticleInclusive

insert ResConstructor:Intermediates 0 /Herwig/FRModel/Particles/~H0
insert ResConstructor:Intermediates 0 /Herwig/FRModel/Particles/~A0
insert ResConstructor:Intermediates 0 /Herwig/FRModel/Particles/~H+
insert ResConstructor:Intermediates 0 /Herwig/FRModel/Particles/~H-

