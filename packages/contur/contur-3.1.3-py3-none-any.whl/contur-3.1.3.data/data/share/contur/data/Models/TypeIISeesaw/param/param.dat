#NEW style parameter reading file
#Based off configObj python template
[Run]
generator = '<path/to/madgraph>/bin/mg5_aMC'
contur ='<path/to/contur>/setupContur.sh'
environment='<path/to/setupenv>/setupEnv.sh'

[Parameters]
[[mdpp]]
mode = CONST
value = __mdpp__
[[gap]]
mode = LIN
start = 40.0
stop = 100.0
number = 13
[[mdp]]
mode = REL
form = {mdpp} + {gap}
