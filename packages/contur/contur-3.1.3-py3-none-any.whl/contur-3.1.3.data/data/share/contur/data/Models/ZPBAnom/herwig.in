# Used with param_file.dat
# to steer the generators for https://arxiv.org/abs/2110.13518

read FRModel.model

set /Herwig/FRModel/Particles/Zp:NominalMass {mzp} 
set /Herwig/FRModel/FRModel:tsb {tsb} 
set /Herwig/FRModel/FRModel:gzp {ccnew} 

cd /Herwig/NewPhysics
insert HPConstructor:Outgoing 0 /Herwig/FRModel/Particles/Zp

insert ResConstructor:Intermediates 0 /Herwig/FRModel/Particles/Zp
