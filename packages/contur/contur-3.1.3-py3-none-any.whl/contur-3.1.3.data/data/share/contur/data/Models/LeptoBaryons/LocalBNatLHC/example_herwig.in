# Read in the Feynrules model
read FRModel.model

# Boson Masses
set /Herwig/FRModel/Particles/ZB:NominalMass {MZB}*GeV
set /Herwig/FRModel/Particles/hB:NominalMass {mhB}*GeV

# Fermion masses
set /Herwig/FRModel/Particles/chi:NominalMass {Mchi}*GeV

# Couplings / Parameters
set /Herwig/FRModel/FRModel:gB {gB}
set /Herwig/FRModel/FRModel:sth {sth}

cd /Herwig/NewPhysics
# set the outgoing particles for the inclusive process
insert HPConstructor:Outgoing 0 /Herwig/FRModel/Particles/ZB
insert HPConstructor:Outgoing 0 /Herwig/FRModel/Particles/hB
insert HPConstructor:Outgoing 0 /Herwig/FRModel/Particles/chi

# set the intermediate particles for the resonant process
insert ResConstructor:Intermediates 0 /Herwig/FRModel/Particles/ZB
insert ResConstructor:Intermediates 0 /Herwig/FRModel/Particles/hB

