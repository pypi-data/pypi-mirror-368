# avoid generating lots on low pT stuff
set /Herwig/Cuts/JetKtCut:MinKT 50*GeV
set /Herwig/Shower/ShowerHandler:Interactions QCD

# Read in the Feynrules model
read FRModel.model

set /Herwig/FRModel/Particles/N1:NominalMass {mn1}*GeV
set /Herwig/FRModel/FRModel:cLNH {clnh}
set /Herwig/FRModel/FRModel:cNNH {cnnh}
set /Herwig/FRModel/FRModel:cNA {cna}
set /Herwig/FRModel/FRModel:LambdaSMEFT {lambda}

cd /Herwig/NewPhysics
insert HPConstructor:Outgoing 0 /Herwig/FRModel/Particles/N1
# insert HPConstructor:Outgoing 0 /Herwig/FRModel/Particles/H

