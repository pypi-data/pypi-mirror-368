structure math
{
    static int pow(int base, int exp)
    {
        if (exp == 0) {
            return 1;
        }
        return base * math.pow(base, exp - 1);
    }

    static int fibonnacci(int number)
    {
        if (number <= 2) {
            return 1;
        }
        return math.fibonnacci(number - 1) + math.fibonnacci(number - 2);
    }
}
