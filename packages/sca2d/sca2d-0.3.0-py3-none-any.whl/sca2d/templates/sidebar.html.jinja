<input type="checkbox" id="nav-toggle" hidden>
<label for="nav-toggle" class="menu-icon"></label>
<nav class="sidebar">
  <h2>{{ breakable_page_title }}</h2>
  <ul>
    {% for definition in definitions %}
    <li><a href="#{{ definition.id }}">{{ definition.breakable_id }}</a></li>
    {% endfor %}
  </ul>
  {% if project_title is none %}
  <h2><a href="{{rel_root}}/index.html">
    Index
  </a></h2>
  <h3>
  .scad files
  </h3>
  {% else %}
  <h2><a href="{{rel_root}}/index.html">
  {{ project_title }}
  </a></h2>
  {% endif %}
  <ul>
    {% for doc_path in doc_paths %}
    {% if page_title ==  doc_path.id %}
    <li class="active-nav"><a href="{{ doc_path.url }}">{{ doc_path.breakable_id }}</a></li>
    {% else %}
    <li><a href="{{ doc_path.url }}">{{ doc_path.breakable_id }}</a></li>
    {% endif %}
    {% endfor %}
  </ul>
</nav>
  