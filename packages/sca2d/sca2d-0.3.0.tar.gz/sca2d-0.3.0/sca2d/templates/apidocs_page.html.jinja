{% extends "default.html.jinja" %}

{% block content %}
<div class="file-docs">
    {{ file_docs }}
</div>
{% for definition in definitions %}
<div class="definition-block" id="{{ definition.id }}">
    <div class="definition-head">
    {{ definition.signature }}
    </div>
    <details class="source-view">
    <summary>[Source]</summary>
    {{ definition.source }}
    </details>
    <div class="definition-docs">
    {{ definition.docs }}
    </div>
</div>
{% endfor %}
{% endblock %}