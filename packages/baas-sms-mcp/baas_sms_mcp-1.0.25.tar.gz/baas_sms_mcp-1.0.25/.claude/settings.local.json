{
  "$schema": "https://json.schemastore.org/claude-code-settings.json",
  "permissions": {
    "allow": [
      "Bash(python:*)",
      "Bash(rm:*)",
      "Bash(uv build:*)",
      "Bash(sed:*)",
      "Bash(npm pack:*)",
      "Bash(pkill:*)",
      "Bash(BAAS_API_KEY=\"test\" PROJECT_ID=\"test\" gtimeout 5s node index.js)",
      "Bash(npm run release:patch:*)",
      "Bash(curl:*)",
      "Bash(git push:*)",
      "Bash(git config:*)",
      "Bash(gh auth:*)",
      "Bash(git credential-osxkeychain:*)",
      "Bash(security delete-internet-password:*)",
      "Bash(git add:*)",
      "Bash(git commit:*)",
      "Bash(chmod:*)"
    ],
    "deny": []
  }
}